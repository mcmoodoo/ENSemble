# WalletConnect (required for connect button)
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_project_id

# Ethereum mainnet RPC (required for profile/ENS in the browser)
# Must be NEXT_PUBLIC_ so Next.js exposes it to the client; otherwise you get default RPC (e.g. merkle.io) and requests fail.
NEXT_PUBLIC_INFURA_ETHEREUM_MAINNET_RPC=https://mainnet.infura.io/v3/YOUR_INFURA_KEY
